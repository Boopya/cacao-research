   "source": [
    "Create a function that moves the specified number of files from lake_path to processed_path."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "def move_num_of_files(count, lake_path, processed_path, count_end):\n",
    "    try:\n",
    "        images = os.listdir(lake_path)\n",
    "        for file in images:\n",
    "            if count < count_end:\n",
    "                path = f\"{lake_path}/{file}\"\n",
    "                shutil.move(path, processed_path)\n",
    "                count += 1\n",
    "    except Exception as e:\n",
    "        print(e)"
   ]
  },
  {
   "attachments": {},
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Move 20% of each class into val directory."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "black_pod_20 = math.ceil(len(black_pod_rot) * 0.20)\n",
    "healthy_20 = math.ceil(len(healthy) * 0.20)\n",
    "pod_borer_20 = math.ceil(len(pod_borer) * 0.20)\n",
    "\n",
    "move_num_of_files(0, images_path+\"/black_pod_rot\", \"../val/black_pod_rot/\", black_pod_20)\n",
    "move_num_of_files(0, images_path+\"/healthy\", \"../val/healthy/\", healthy_20)\n",
    "move_num_of_files(0, images_path+\"/pod_borer\", \"../val/pod_borer/\", pod_borer_20)"
   ]
